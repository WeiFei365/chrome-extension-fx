{
  "manifest_version": 3,

  "name": "FX网页小公举",
  "description": "用于裁剪网页、全屏打印图片、文字拷贝等",
  "version": "1.1",

  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "FX Element Cut"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}
